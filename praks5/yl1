#!/bin/bash

#KÃ¼sitakse kasutajalt vanust
read -p "Sisesta oma vanus: " vanus

# Kontrollime, kas sisend on number
if ! [[ "$vanus" =~ ^[0-9]+$ ]]; then
    echo "Vigane sisend! Palun sisesta ainult positiivne tÃ¤isarv."
    exit 1
fi

# Vanuse kategooriate kontroll
if (( vanus >= 0 && vanus <= 11 )); then
    echo "Oled laps."
elif (( vanus >= 12 && vanus <= 18 )); then
    echo "Oled nooruk."
elif (( vanus >= 19 && vanus <= 63 )); then
    echo "Oled tÃ¤iskasvanu."
else
    echo "Oled senjoor."
fi
